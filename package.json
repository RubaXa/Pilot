{
  "name": "pilotjs",
  "version": "2.12.0",
  "devDependencies": {
    "@types/jest": "23.3.13",
    "amdclean": "^2.7.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-amd-to-commonjs": "^1.2.0",
    "babel-plugin-transform-string-literal-replace": "^1.0.2",
    "grunt": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-requirejs": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.0",
    "grunt-qunit-istanbul": "^1.0.0",
    "grunt-version": "*",
    "jest": "^23.6.0",
    "request": "^2.65.0"
  },
  "description": "Multifunction JavaScript router.",
  "main": "Pilot.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "grunt build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rubaxa/Pilot.git"
  },
  "keywords": [
    "Pilot",
    "router",
    "routing",
    "route"
  ],
  "author": "Konstantin Lebedev <ibnRubaXa@gmail.com>",
  "license": "MIT",
  "include": [
    "Pilot.js"
  ],
  "jest": {
    "testRegex": "\\.tests\\.js$",
    "transform": {
      "\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "report",
    "collectCoverageFrom": [
      "src/*.js"
    ]
  }
}
